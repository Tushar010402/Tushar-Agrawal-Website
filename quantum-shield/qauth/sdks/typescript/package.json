{
  "name": "@quantumshield/qauth",
  "version": "0.1.0",
  "description": "QuantumAuth - Next-generation authentication and authorization protocol with post-quantum security",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:wasm": "cd ../../rust && wasm-pack build --target web --out-dir ../sdks/typescript/wasm --features wasm",
    "test": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build:wasm && npm run build"
  },
  "keywords": [
    "authentication",
    "authorization",
    "post-quantum",
    "security",
    "oauth",
    "jwt",
    "qauth",
    "quantum-safe"
  ],
  "author": "Tushar Agrawal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tushar-agrawal/quantum-shield"
  },
  "homepage": "https://tusharagrawal.in/quantum-auth",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
