    "cascading-encryption",
    "defense-in-depth",
    "security",
    "pqc"
  ],
  "author": "Tushar Agrawal",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
